<% if @events %>
<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Start at</th>
      <th>End at</th>
    </tr>
  </thead>
  <tbody>
  <% @events.each do |e| %>
    <%= erb :"events/_event_row", layout: false, :locals => { :e => e } %>
  <% end %>
  </tbody>
</table>
<% end %>
